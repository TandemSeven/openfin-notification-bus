                  }
                },
                {
                  "type": "component",
                  "componentName": "view",
                  "componentState": {
                    "name": "contact",
                    "url": "http://localhost:3000/contact"
                  }
                }
              ]
            }
          ]
        }
      },
      {
        "defaultWidth": 600,
        "defaultHeight": 600,
        "defaultLeft": 200,
        "defaultTop": 200,
        "layout": {
          "content": [
            {
              "type": "stack",
              "content": [
                {
                  "type": "component",
                  "componentName": "view",
                  "componentState": {
                    "name": "about",
                    "url": "http://localhost:3000/about"
                  }
                }
              ]
            }
          ]
        }
      }
    ]
